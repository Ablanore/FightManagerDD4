{
    "compilerOptions": {
        "module": "ES2020",
        "moduleResolution": "node",
        "outDir": "dist",
        "target": "ES2020",
        "noEmitOnError": true,
        "strict": true,
        "allowSyntheticDefaultImports":true,
        "resolveJsonModule": true
    },
    "files": [
        "src/app.ts"
    ],
    "include": [
        "src/model/*.ts"
, "src/model/race.ts"    ],
    "resolveJsonModule": true,
    "esModuleInterop": true
}